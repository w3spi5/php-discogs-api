{
    "name": "w3spi5/php-discogs-api",
    "description": "The Discogs API makes it easy for developers to communicate with the Discogs platform",
    "type": "library",
    "keywords": ["discogs", "api"],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0 || ^8.0",
        "guzzlehttp/guzzle-services": "^1.2",
        "guzzlehttp/oauth-subscriber": "^0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.23 || ^9.0",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Richard van den Brand",
            "email": "richard@vandenbrand.org"
        },
        {
            "name": "Wespify Solutions",
            "email": "naetheria@wespify.com"
        }
    ],
    "autoload": {
        "psr-0": { "Discogs": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "suggest": {
        "w3spi5/discogs-bundle": "For integrating Discogs into Symfony 5",
        "ricbra/discogs-bundle": "For integrating Discogs into Symfony 2"
    },
    "scripts":{
        "test": "phpunit tests"
    }
}
